# API Server Configuration
# Port sur lequel l'API REST va écouter
PORT=5000

# Host sur lequel l'API REST va écouter (0.0.0.0 pour accepter toutes les connexions)
HOST=0.0.0.0

# Environment (development, production, test)
NODE_ENV=development

# Logging
LOG_LEVEL=info

# Redis Cache Configuration
REDIS_ENABLED=false
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_TIMEWINDOW=60000
